.img_item {
  width: 48%;
  margin: 1%;
  display: inline-block;
  vertical-align: top;
}
 .buttom{  
  width: 10vw;
  height: 10vw;
  bottom: 4vw;  
  right: 4vw;
  position: fixed;   
  display: inline-block;  
  border: 5vw;   
  border-radius: 5vw 
} 
.icon_kf{  
  width: 10vw;  
  height: 10vw;
  display: inline-block;
  border-radius: 5vw 
}
.modal-box{
  position:absolute; 
  width:100%;
  height:100%;
  bottom:0;
  background:rgba(0,0,0,0.4);
  overflow: hidden;
  }
.phone{
  position:absolute; 
  width: 100%;
  height: 11vw;
  bottom: 0;
  left: 0vw;
  background-color: white;
  line-height:82rpx;
}
.phone text {
  vertical-align:middle;
  text-align: center;
  display: table-cell;
}
.address{
  position:absolute; 
  width: 100%;
  bottom: 11.1vw;
  left: 0vw;
  background-color: white;
  padding:0%;
  line-height:82rpx;
}
.address text {
  text-align: center;
  vertical-align:middle;
  display: table-cell;
}
